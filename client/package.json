{
  "name": "client",
  "version": "1.0.11",
  "private": true,
  "scripts": {
    "test": "node scripts/test.js",
    "start:client": "node scripts/start.js",
    "start:project": "concurrently \"cd ../server && npm run start:server\" \"node scripts/start.js\"",
    "start:prod": "cd ../dist && node server.js",
    "build:client": "node scripts/build.js",
    "build:project": "concurrently \"cd ../server && npm run build:server\" \"node scripts/build.js\"",
    "package": "npm run build:project && copyfiles -E -V -u 3 \"../server/dist/*\" ../dist && copyfiles -E -V -u 1 \"dist/**/*\" ../dist/public",
    "prettier": "prettier --write --config .prettierrc src/**/*",
    "prettier:check": "prettier --check --config .prettierrc src/**/*",
    "lint": "eslint src src --ext .tsx",
    "lint:fix": "eslint src src --ext .tsx --fix",
    "lint:quiet": "eslint src src --ext .tsx --quiet"
  },
  "dependencies": {
    "@azure/communication-calling": "1.4.4",
    "@azure/communication-chat": "1.1.0",
    "@azure/communication-identity": "^1.0.0",
    "@azure/communication-react": "1.2.0",
    "bfj": "^7.0.2",
    "camelcase": "^6.1.0",
    "http-errors": "1.8.1",
    "prompts": "2.4.0",
    "react": "^16.14.0",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^16.14.0",
    "react-refresh": "^0.11.0",
    "resolve": "1.18.1",
    "scheduler": "^0.20.1",
    "semver": "7.3.2",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@fluentui/react": "^8.36.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@svgr/webpack": "^6.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.17",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chokidar": "^3.5.2",
    "concurrently": "^6.2.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "eslint-webpack-plugin": "^2.5.2",
    "file-loader": "^6.1.1",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.6.0",
    "jest-circus": "26.6.0",
    "jest-resolve": "26.6.0",
    "jest-watch-typeahead": "0.6.1",
    "mini-css-extract-plugin": "0.11.3",
    "mobile-detect": "1.4.5",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "2.0.5",
    "react-dev-utils": "^12.0.0",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.0.5",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.4.4",
    "ts-pnp": "^1.2.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^6.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
