{
  "name": "server",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "start:server": "ts-node-dev ./bin/www",
    "start:project": "concurrently \"npm run start:server\" \"cd ../client && npm run start:client\"",
    "start:prod": "cd ../dist && node server.js",
    "build:server": "webpack",
    "build:project": "concurrently \"npm run build:server\" \"cd ../client && npm run build:client\"",
    "package": "npm run build:project && copyfiles -E -V -u 1 \"dist/*\" ../dist && copyfiles -E -V -u 3 \"../client/dist/**/*\" ../dist/public",
    "prettier": "prettier --write --config .prettierrc src/**/*",
    "prettier:check": "prettier --check --config .prettierrc src/**/*",
    "lint": "eslint src src --ext .ts",
    "lint:fix": "eslint src src --ext .ts --fix",
    "lint:quiet": "eslint src src --ext .ts --quiet"
  },
  "dependencies": {
    "@azure/communication-chat": "^1.5.4",
    "@azure/communication-common": "^2.3.1",
    "@azure/communication-identity": "^1.3.0",
    "@azure/communication-rooms": "^1.2.0",
    "@azure/communication-call-automation": "1.4.0-beta.1",
    "@azure/core-http": "3.0.5",
    "@azure/cosmos": "^3.17.0",
    "@types/cors": "^2.8.8",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "eslint-plugin-header": "^3.1.0",
    "express": "~4.21.2",
    "express-rate-limit": "7.5.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/express": "^4.17.8",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/node": "^22.14.0",
    "@types/supertest": "^2.0.10",
    "@types/webpack": "^5.28.1",
    "@types/ws": "^8.5.12",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "concurrently": "^6.2.2",
    "copy-webpack-plugin": "^6.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "29.7.0",
    "jest-junit": "^13.0.0",
    "json-loader": "^0.5.7",
    "prettier": "2.0.5",
    "pretty-quick": "^3.1.0",
    "supertest": "^6.0.1",
    "serialize-javascript": ">=6.0.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^8.0.12",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "word-wrap": ">=1.2.4",
    "webpack": "5.76.0",
    "webpack-cli": "4.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}